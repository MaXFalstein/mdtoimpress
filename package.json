{
    "name": "markdown-impress",
    "description": "A tool to convert markdown to impress page.",
    "version": "0.1.6",
    "contributors": [
        { "name": "Steel Liao", "email": "lbbsteel@gmail.com"},
        { "name": "Rich Boakes", "email": "rjb@port.ac.uk"}
    ],
    "bugs": "https://github.com/ear1grey/markdown-impress/issues",
    "dependencies": {
        "marked": "*",
        "commander": "*"
    },
    "engines": {
